{
  "name": "gmmcal.com.br",
  "private": true,
  "description": "My personal website code, playground and learning lab.",
  "version": "2.0.0",
  "homepage": "https://www.gmmcal.com.br",
  "repository": "https://github.com/gmmcal/gmmcal.com.br",
  "bugs": {
    "url": "https://github.com/gmmcal/gmmcal.com.br/issues",
    "email": "bugs@gmmcal.com.br"
  },
  "author": {
    "name": "Gustavo Cunha",
    "email": "contact@gmmcal.com.br",
    "url": "https://www.gmmcal.com.br"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/gmmcal/gmmcal.com.br/blob/master/LICENSE"
    }
  ],
  "contributors": [
    {
      "name": "Gustavo Cunha",
      "email": "contact@gmmcal.com.br",
      "url": "https://www.gmmcal.com.br"
    }
  ],
  "keywords": [
    "ruby",
    "rails",
    "vuejs2",
    "jest",
    "rspec",
    "ruby-on-rails",
    "cypress",
    "end-to-end-testing",
    "end-to-end",
    "travis-ci",
    "ebert",
    "codeclimate",
    "hakiri",
    "heroku",
    "ci-cd"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.4.0",
    "yarn": ">=1.10.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "*",
    "@fortawesome/free-brands-svg-icons": "*",
    "@fortawesome/free-regular-svg-icons": "*",
    "@fortawesome/free-solid-svg-icons": "*",
    "@fortawesome/vue-fontawesome": "*",
    "@rails/webpacker": "3.5",
    "bootstrap": "*",
    "i18n-js": "*",
    "marked": "*",
    "rails-ujs": "*",
    "sortablejs": "*",
    "turbolinks": "*",
    "vue": "*",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "*",
    "vue-turbolinks": "*",
    "yarn": "*"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-jest": "*",
    "cypress": "*",
    "jest": "*",
    "jest-canvas-mock": "*",
    "jest-serializer-vue": "*",
    "vue-jest": "*",
    "webpack-dev-server": "2.11.2"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "spec/frontend/javascript",
      "app/javascript/packs"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/javascript/packs/components/**/*.{js,vue}",
      "app/javascript/packs/functional/**/*.{js}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "lcovonly",
      "html"
    ],
    "coverageDirectory": "coverage/frontend",
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/javascript/packs/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "scripts": {
    "test": "jest --watch",
    "test:once": "jest",
    "cypress": "rake db:setup && cypress open",
    "cypress:ci": "cypress run --record"
  }
}
